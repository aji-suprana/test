{
  "name": "finance_book-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "SET DEBUG=finance_book-service:* & nodemon ./bin/www",
    "createdb": "npx sequelize-cli db:create",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "test:setup": "cross-env NODE_ENV=test npm run createdb && cross-env NODE_ENV=test npm run migrate:reset",
    "test": "jest --detectOpenHandles --force-exit"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cli-interact": "^0.1.9",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.6.0",
    "flaverr": "^1.10.0",
    "fs": "0.0.1-security",
    "helmet": "^3.23.3",
    "jest-serial-runner": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "path": "^0.12.7",
    "pg": "^8.2.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.22.3",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "jest": "^26.1.0",
    "nodemon": "^2.0.4"
  }
}
